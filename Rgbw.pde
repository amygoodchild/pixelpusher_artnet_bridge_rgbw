
// possibly the most pleasing class ever written

class Rgbw{
  
  float r;
  float g;
  float b;
  float w;
    
  Rgbw(float r_, float g_, float b_, float w_){
    r = r_;
    g = g_;
    b = b_;
    w = w_;
  }
  
  void update(float r_, float g_, float b_, float w_){
    r = r_;
    g = g_;
    b = b_;
    w = w_; 
  }

}
